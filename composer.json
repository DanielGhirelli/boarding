{
    "name": "boarding",
    "description": "Application Boarding Portal",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=8.2",
        "ext-iconv": "*",
        "api-platform/core": "^2.6",
        "aws/aws-sdk-php": "^3.21",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/annotations": "^1.0",
        "doctrine/common": "^3",
        "doctrine/doctrine-bundle": "^2",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.14",
        "enlightn/security-checker": "^1.10",
        "guzzlehttp/psr7": "^1.4",
        "kriswallsmith/buzz": "^1.0",
        "mailgun/mailgun-php": "^4.3",
        "nelmio/cors-bundle": "^2.1",
        "nyholm/psr7": "^1.2",
        "php-http/curl-client": "^2.2",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^4.5",
        "symfony/asset": "^5.4",
        "symfony/browser-kit": "^5.4",
        "symfony/cache": "^5.4",
        "symfony/console": "^5.4",
        "symfony/expression-language": "^5.4",
        "symfony/flex": "^1.18",
        "symfony/form": "^5.4",
        "symfony/framework-bundle": "^5.4",
        "symfony/http-client": "^5.4",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/property-access": "^5.4",
        "symfony/property-info": "^5.4",
        "symfony/security-bundle": "^5.4",
        "symfony/serializer": "^5.4",
        "symfony/validator": "^5.4",
        "symfony/yaml": "^5.4",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpunit/phpunit": "^9.6",
        "rector/rector": "^0.15.18",
        "roave/security-advisories": "dev-latest",
        "symfony/debug-bundle": "^5.4",
        "symfony/dotenv": "5.4.*",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "^5.4",
        "symfony/stopwatch": "^5.4",
        "symfony/var-dumper": "^5.4",
        "symfony/web-profiler-bundle": "^5.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "symfony/flex": true,
            "php-http/discovery": false
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": ["src/", "src/Application", "src/Controller", "src/Entity",
                "src/Exception", "src/Filter", "src/Form", "src/Migrations", "src/Register",
                "src/Repository", "src/Response", "src/Security", "src/Serializer", "src/Service", "src/Validator"]
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "5.4.*"
        }
    }
}
